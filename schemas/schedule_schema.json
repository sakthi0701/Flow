{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Schedule",
  "type": "array",
  "items": {
    "type": "object",
    "required": ["task_id", "title", "start", "end"],
    "properties": {
      "task_id": {"type": "string"},
      "title": {"type": "string"},
      "start": {"type": "string", "format": "date-time"},
      "end": {"type": "string", "format": "date-time"}
    }
  }
}
